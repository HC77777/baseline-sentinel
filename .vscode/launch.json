{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension",
      "type": "extensionHost",
      "request": "launch",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/packages/vscode-baseline-sentinel"
      ],
      "outFiles": [
        "${workspaceFolder}/packages/vscode-baseline-sentinel/dist/**/*.js"
      ],
      "preLaunchTask": "npm: build"
    }
  ]
}